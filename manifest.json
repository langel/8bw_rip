{
	"manifest_version":2,
	"version":"1.0",
	"name":"8BW RIP",
	"content_scripts":[
		{
			"matches":["https://8bitworkshop.com/*"],
			"js":["main.js"]
		}
	],
	"background": {
		"scripts": ["main.js"]
	},
	"permissions": ["tabs"],
	"icons": {
		"48": "icon-48.png",
		"96": "icon-96.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "icon-19.png",
			"38": "icon-38.png"
		},
		"default_title": "8BW RIP"
	}
}
